{
  // json: 프로그래머가 파일에 정보를 저장하기 위해 만든 방식 중 하나
  // 파일명은 무조건 package.json
  // nodeJS 프로젝트를 만들 때 가장 먼저 만들어야 할 파일 
  "name": "youtube_2022",
  "version": "1.0.0",
  "description": "The best way to watch videos.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yejiseo0725/Youtube_2022.git"
  },
  // script: 실행하고 싶은 것  
  "scripts": {
    // babel 을 설치했기 때문에 babel-node 라는 명령어 사용 가능 (최신 문법 코드 사용 가능)
    // nodemon 설치 후, npm run dev 는 한 번만 실행해 주면 수정이 되어도 알아서 재시작 해준다.
    "dev": "nodemon --exec babel-node src/server.js"
  },
  "author": "Yeji Seo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yejiseo0725/Youtube_2022/issues"
  },
  "homepage": "https://github.com/yejiseo0725/Youtube_2022#readme",
  // dependencies & devDependencies 는 npm install --save-dev @babel/core 입력 후 생김.
  "dependencies": {
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "nodemon": "^2.0.15"
  }
}
